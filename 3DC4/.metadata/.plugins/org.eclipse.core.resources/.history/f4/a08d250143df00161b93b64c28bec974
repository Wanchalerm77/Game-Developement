package Model;

public class Disc {

	public enum Color {
		GREEN('g'), RED('r'), BLUE('b'), ORANGE('o'), PURPLE('p'), YELLOW('y');

		public final char c;

		private Color(char c) {
			this.c = c;
		}
	}

	private Color color;

	public Disc(Color color) {
		this.color = color;
	}

	public Color getColor() {
		return color;
	}

}
