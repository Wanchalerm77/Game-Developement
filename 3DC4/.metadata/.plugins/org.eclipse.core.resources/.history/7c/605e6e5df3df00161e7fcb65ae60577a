package Model;

public enum Disc {


		WHITE('w'), GREEN('g'), RED('r'), BLUE('b'), ORANGE('o'), PURPLE('p'), YELLOW('y'), EMPTY('E');

		public final char c;

		private Disc(char c) {
			this.c = c;
		}
	}

	private Color color;

	public Disc(Color color) {
		this.color = color;
	}

	public Disc.Color getColor() {
		return col;
	}

}
