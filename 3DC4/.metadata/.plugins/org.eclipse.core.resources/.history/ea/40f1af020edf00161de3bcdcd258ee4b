import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStream;
import java.io.InputStreamReader;
import java.io.OutputStream;
import java.io.OutputStreamWriter;

public class TestScanner {
	public static void main(String[] args) throws IOException {
		/*
		 * Scanner in = new Scanner(System.in);
		 * 
		 * System.out.print(" What is your name?"); String name = in.nextLine();
		 * 
		 * System.out.print("What is your age?"); int age = in.nextInt();
		 */

		// System.out.println("Hello" + " " + args[0] + ", you are " + args[1] +
		// " years old....congrats?");
		BufferedReader in = new BufferedReader(new InputStreamReader(new InputStream() {

			@Override
			public int read() throws IOException {
				// TODO Auto-generated method stub
				return 0;
			}
		}));
		BufferedWriter out = new BufferedWriter(new OutputStreamWriter(new OutputStream() {

			public void write(String b) throws IOException {
				System.out.println(b);

			}

			@Override
			public void write(int b) throws IOException {
				// TODO Auto-generated method stub

			}
		}));

		out.write("Lol");

	}
}
