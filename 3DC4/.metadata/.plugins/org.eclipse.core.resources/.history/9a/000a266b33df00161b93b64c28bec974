package Model;

public enum ColorOfPlayer {

	GREY("142, 142, 147"), RED("255, 59, 48"), GREEN("76, 217, 100"), PURPLE("88, 86, 214"), LIGHTBLUE("52, 170, 220");

	private ColorOfPlayer(final int r,final int g,final int b) {
        this.r = r;
        this.g = g;
        this.b = b;
        this.rgb = r + ", " + g + ", " + b;
	
	
	
	
	
	

}
